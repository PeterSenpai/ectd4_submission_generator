{
    "name": "ectd-generator",
    "version": "1.0.0",
    "description": "eCTD 4.0 Submission Generator - Full Stack Application",
    "private": true,
    "scripts": {
        "backend:install": "cd backend && npm install",
        "backend:start": "cd backend && npm run start:server",
        "backend:dev": "cd backend && npm run dev:server",
        "backend:generate": "cd backend && npm run generate",
        "frontend:install": "cd frontend && npm install",
        "frontend:start": "cd frontend && npm run dev",
        "frontend:build": "cd frontend && npm run build",
        "install:all": "npm run backend:install && npm run frontend:install",
        "dev": "echo 'Run backend:start and frontend:start in separate terminals'"
    },
    "keywords": [
        "ectd",
        "fda",
        "regulatory",
        "submission"
    ],
    "license": "MIT"
}
